datasource db {
    provider = "mongodb"
    url      = env("DATABASE_URL")
}

generator client {
    provider = "prisma-client-js"
}

model urls {
    id     String  @id @default(auto()) @map("_id") @db.ObjectId
    URL    String
    domain String
    hash   String? @unique
    alias  String?
    author String?
}
